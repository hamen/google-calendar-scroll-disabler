{
  "manifest_version": 2,
  "name": "Google Calendar Scroll Disabler",
  "version": "0.4",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/calendar/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
